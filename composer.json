{
  "name": "suretly/php7-sdk",
  "description": "PHP SDK for Suretly Lender API",
  "keywords": [
    "lenderAPI",
    "suretly",
    "php-sdk"
  ],
  "type": "library",
  "require": {
    "php": "^7.1.3",
    "guzzlehttp/guzzle": "^6.3",
    "netresearch/jsonmapper": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "symfony/var-dumper": "^4.1"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksandr Serenko",
      "email": "fafnur@yandex.ru"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Suretly\\LenderApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Suretly\\LenderApi\\Tests\\": "tests/"
    }
  }
}
